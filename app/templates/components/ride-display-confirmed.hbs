<div class="row" style="margin-top:20px;">
    <div class='col-md-2' style="margin-right: 15px;">{{moment-format ride.date 'MM/DD/YYYY' 'MM-DD-YYYY'}}</div>   
    <div class='col-md-2' style="margin-right: 15px;">{{ride.time}}</div>
    <div class='col-md-2' style="margin-right: 15px;">{{ride.pickup}}</div>
    <div class='col-md-2' style="margin-right: 10px;">{{ride.dropoff}}</div>
    <div class='col-md-1' style="margin-right: 15px;">{{ride.status}}</div>
    <div class='col-md-1'>{{ride.token}}</div>
    
</div>
    {{yield}}
